{{#each colors}}
$md-colors-{{@key}}: (
{{#each this}}
  {{@key}}: {{this}},
{{/each}}
);

{{/each}}
$md-colors: (
{{#each colors}}
  {{@key}}: $md-colors-{{@key}},
{{/each}}
);

@function md-color($pallete, $color) {
  $map: map-get($md-colors, $pallete);
  $hex: map-get($map, $color);
  @return $hex;
}
